local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Handshake = ReplicatedStorage.Remotes.CharacterSoundEvent
local Hooks = {}
local HandshakeInts = {}
local BannedPlayers = {}
local BanDuration = 2592000 -- 1 month in seconds
local WEBHOOK_URL = "YOUR_WEBHOOK_URL" -- Replace with your webhook URL

local function deepFreeze(obj)
    if type(obj) ~= "table" then
        return obj
    end
    local frozen = {}
    for k, v in pairs(obj) do
        frozen[deepFreeze(k)] = deepFreeze(v)
    end
    return setmetatable(frozen, {
        __index = function(t, k)
            return rawget(t, k)
        end,
        __newindex = function(t, k, v)
            error("Cannot modify frozen table", 2)
        end,
        __metatable = false,
    })
end

local function isFunctionTampered(func)
    if typeof(func) ~= "function" then
        return false
    end
    local info = debug.info(func, "s")
    if not info then
        return false
    end
    local upvalues = debug.getupvalues(func)
    for _, upvalue in ipairs(upvalues) do
        if type(upvalue) == "function" and debug.info(upvalue, "s") then
            return true
        end
    end
    return false
end

local function monitorFunction(func)
    if typeof(func) ~= "function" then
        return
    end
    local original = func
    local proxy = newproxy(true)
    local meta = {
        __call = function(_, ...)
            if isFunctionTampered(original) then
                local player = game:GetService("Players").LocalPlayer
                local playerId = player.UserId
                if BannedPlayers[playerId] then
                    BannedPlayers[playerId] = os.time() + math.huge
                    player:Kick("Permanent ban for cheating.")
                else
                    BannedPlayers[playerId] = os.time() + BanDuration
                    player:Kick("Banned for 1 month for cheating.")
                end
                
                local success, err = pcall(function()
                    local HttpService = game:GetService("HttpService")
                    local data = HttpService:JSONEncode(BannedPlayers)
                    HttpService:PostAsync(WEBHOOK_URL, data)
                end)
                if not success then
                    warn("Failed to send ban data:", err)
                end
                error("Function Tampering Detected", 2)
            end
            return original(...)
        end,
        __metatable = false,
    }
    setmetatable(proxy, meta)
    return proxy
end

local function obfuscateString(str)
    local obfuscated = ""
    for i = 1, #str do
        obfuscated = obfuscated .. string.char(string.byte(str, i) + 5)
    end
    return obfuscated
end

local function deobfuscateString(str)
    local deobfuscated = ""
    for i = 1, #str do
        deobfuscated = deobfuscated .. string.char(string.byte(str, i) - 5)
    end
    return deobfuscated
end

local function secureHookFunction(func, hook)
    local original = func
    local proxy = newproxy(true)
    local meta = {
        __call = function(_, ...)
            local result = hook(original, ...)
            return result
        end,
        __metatable = false,
    }
    setmetatable(proxy, meta)
    return proxy
end

local function checkAndBan(player, reason)
    local playerId = player.UserId
    if BannedPlayers[playerId] then
        BannedPlayers[playerId] = os.time() + math.huge
        player:Kick("Permanent ban for " .. reason)
    else
        BannedPlayers[playerId] = os.time() + BanDuration
        player:Kick("Banned for 1 month for " .. reason)
    end
    
    local success, err = pcall(function()
        local HttpService = game:GetService("HttpService")
        local data = HttpService:JSONEncode(BannedPlayers)
        HttpService:PostAsync(WEBHOOK_URL, data)
    end)
    if not success then
        warn("Failed to send ban data:", err)
    end
end

local function monitorGameFunctions()
    for i, v in getgc() do
        if typeof(v) == "function" and islclosure(v) then
            if (#getprotos(v) == 1) and table.find(getconstants(getproto(v, 1)), 4000001) then
                local hook = secureHookFunction(v, function(original, ...)
                    local player = game:GetService("Players").LocalPlayer
                    checkAndBan(player, "cheating (Anti-Cheat Function Hooked)")
                    error("Anti-Cheat Function Hooked", 2)
                    return original(...)
                end)
                
                
            end
        end
    end
end

monitorGameFunctions()

Hooks.__namecall = hookmetamethod(game, "__namecall", function(self, ...)
    local Method = getnamecallmethod()
    local Args = {...}

    if not checkcaller() and (self == Handshake) and (Method == "fireServer") and (string.find(Args[1], "AC")) then
        if (#HandshakeInts == 0) then
            HandshakeInts = {table.unpack(Args[2], 2, 18)}
        else
            for i, v in HandshakeInts do
                Args[2][i + 1] = v
            end
        end
    end

    return Hooks.__namecall(self, ...)
end)

