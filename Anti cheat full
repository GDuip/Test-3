    math.random(1000000, 100000000),
    math.random(1000000, 100000000),
    math.random(1000000, 100000000),
    math.random(1000000, 100000000),
    math.random(1000000, 100000000),
    newproxy(true)
}, {__call = __call})

Remote.OnClientEvent:Connect(function(Method, _, NewArgs)
    if Method == "💱AC" then
        if NewArgs then
            for i = 1, #NewArgs do
                Handshake[i + 1] = NewArgs[i]
            end
        end
    end
end)

task.wait(4)

