local function __call(T, v1, v2, v3, v4, v5, v6)
    warn(v1, v2, v3, v4, v5, v6)
    if ((v1 and v1 ~= v2) and v2 ~= v3) then
        T[math.round(((T[11] / 90) ^ (1 / 3) - 112) / -9) + 1] = T[math.floor(math.sqrt(v3 - 555))]
        return T
    end
end

